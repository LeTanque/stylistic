{
  "author": "letanque",
  "name": "stylistic-letanque",
  "description": "stylistic by letanque",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "update_url": "https://github.com/letanque/stylistic",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_title": "Boilerplate popup!",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "in-content.js"
      ],
      "css": [
        "compiled.css"
      ]
    }
  ],
  "background_page": "background.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src * data: 'self' 'unsafe-eval'",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab"
  ]
}